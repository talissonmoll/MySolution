<?xml version="1.0" encoding="utf-8" ?>
<Options xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Applications>
		<!-- Web -->
		<Application
			Name="MySolutionWeb"
			UseIISExpress="True"
			Url="http://localhost:4030"
			PhysicalPath="[WebAppBin]"
			AdapterFileName="[WebAdapterAssemblyPath]"
			IgnoreCase="true"/>
		<!-- For IIS -->
		<!--
		<Application
			Name="MySolutionWeb"
			Url="http://localhost/MySolution.Web/Default.aspx"
			AdapterFileName="[WebAdapterAssemblyPath]"
			IgnoreCase="true"/>
		-->
	</Applications>
	<TestDatabases>
		<Database xsi:type="TestMSSQLDatabase" Server="(localdb)\mssqllocaldb" DBName="MySolutionEasyTest"/>
	</TestDatabases>

	<Aliases>
		<Alias Name="WebAdapterAssemblyPath" Value="[WebAppBin]\Bin\DevExpress.ExpressApp.EasyTest.SeleniumWebAdapter.v21.2.dll" />
		<Alias Name="WebAppBin" Value="[ConfigPath]\..\..\MySolution.Web" />
	</Aliases>
</Options>
